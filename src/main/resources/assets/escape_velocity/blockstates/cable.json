{
  "multipart": [
    {
      "apply": {
        "model": "escape_velocity:block/cable_core"
      }
    },
    {
      "when": { "north": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_north"
      }
    },
    {
      "when": { "south": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_south"
      }
    },
    {
      "when": { "east": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_east"
      }
    },
    {
      "when": { "west": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_west"
      }
    },
    {
      "when": { "up": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_up"
      }
    },
    {
      "when": { "down": "true" },
      "apply": {
        "model": "escape_velocity:block/cable_down"
      }
    }
  ]
}
